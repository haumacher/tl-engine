<?xml version='1.0' encoding='utf-8'?>

<t:template xmlns:t="http://www.top-logic.com/ns/template/1.0">
	<t:head>
		<t:settings output-format="text" />
		<t:types>
			<t:type name="Example_Type">
				<t:attributes>
					<t:attribute name="Example_Attribute" type="string" />
				</t:attributes>
			</t:type>
		</t:types>
		<t:parameters>
			
			<t:parameter name="Example_Parameter" type="Example_Type" />
			
		</t:parameters>
	</t:head>
	<t:body>
		
		<t:define var="Example_Variable" expr="$Example_Parameter.Example_Attribute" />
		
		<t:value expr="Example_Variable" />
		<t:value expr="Example_Variable" />
		<t:value expr="Example_Variable" />
		
	</t:body>
</t:template>
