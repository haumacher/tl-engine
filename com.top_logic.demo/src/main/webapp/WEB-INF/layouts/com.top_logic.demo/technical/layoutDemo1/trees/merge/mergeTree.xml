<?xml version="1.0" encoding="utf-8" ?>

<!--
	Example how to use a MergeTree
	(Be Aware that Merge Trees usually are inner steps
	of an Assistent)
-->
<layout>
	<components>
		<component class="com.top_logic.demo.knowledge.test.layout.MergeTreeDemo"
			buttonComponent="mergeTreeDemoButtons"
			name="mergeTreeDemo"
			page=""
			resPrefix="layouts.layoutdemo.tree.mergeTree.mergeTreeDemo."
		>
			<buttons>
				<button id="performMergeTree"
					class="com.top_logic.base.merge.layout.MergePerformHandler"
				/>
			</buttons>
		</component>
		<buttonbar name="mergeTreeDemoButtons"/>
	</components>
</layout>