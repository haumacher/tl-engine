<?xml version="1.0" encoding="utf-8" ?>

<!--
	Configuration file for MySQL server with version less or equal to 5.5.
	
-->
<application>
	<services>
		<config service-class="com.top_logic.basic.sql.ConnectionPoolRegistry">
			<instance>
				<pools>
					<pool name="default">
						<sql-dialect class="com.top_logic.basic.sql.MySQL55Helper"/>
					</pool>
				</pools>
			</instance>
		</config>
	</services>
</application>