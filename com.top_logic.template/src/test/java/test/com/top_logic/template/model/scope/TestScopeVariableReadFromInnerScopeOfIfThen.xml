<?xml version='1.0' encoding='utf-8'?>

<t:template xmlns:t="http://www.top-logic.com/ns/template/1.0">
	<t:head>
		<t:settings output-format="text" />
	</t:head>
	<t:body>
		
		<t:define var="Example_Variable" expr='"Example Value"' />
		<t:value expr="Example_Variable" />
		<t:if condition="true">
			<t:then>
				<t:value expr="Example_Variable" />
			</t:then>
		</t:if>
		<t:value expr="Example_Variable" />
		
	</t:body>
</t:template>
