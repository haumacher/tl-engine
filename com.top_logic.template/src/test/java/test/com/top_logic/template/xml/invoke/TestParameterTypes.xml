<?xml version='1.0' encoding='utf-8'?>

<t:template xmlns:t="http://www.top-logic.com/ns/template/1.0">
	<t:head>
		<t:settings output-format="text" />
		<t:parameters>
			
			<t:parameter name="Example_Attribute_Type_String" type="string" />
			
			<t:parameter name="Example_Attribute_Type_Xml" type="xml" />
			
			<t:parameter name="Example_Attribute_Type_Boolean" type="boolean" />
			
			<t:parameter name="Example_Attribute_Type_Integer" type="integer" />
			
			<t:parameter name="Example_Attribute_Type_Float" type="float" />
			
			<t:parameter name="Example_Attribute_Type_Date" type="date" />
			
		</t:parameters>
	</t:head>
	<t:body>
		
		<t:value expr='$Example_Attribute_Type_String' />
		
		<t:value expr='$Example_Attribute_Type_Xml' />
		
		<t:value expr='$Example_Attribute_Type_Boolean' />
		
		<t:value expr='$Example_Attribute_Type_Integer' />
		
		<t:value expr='$Example_Attribute_Type_Float' />
		
		<t:value expr='$Example_Attribute_Type_Date' />
		
	</t:body>
</t:template>
