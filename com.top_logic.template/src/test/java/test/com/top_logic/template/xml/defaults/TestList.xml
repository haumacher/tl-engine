<?xml version='1.0' encoding='utf-8'?>

<t:template xmlns:t="http://www.top-logic.com/ns/template/1.0">
	<t:head>
		<t:settings output-format="text" />
		<t:parameters>
			
			<t:parameter name="Example_List_String" multiple="true" type="string">
				<t:default>
					<t:entry value="Delta Goodrem"/>
					<t:entry value="Amy Studt"/>
				</t:default>
			</t:parameter>
			
			<t:parameter name="Example_List_Xml" multiple="true" type="xml">
				<t:default>
					<t:entry><alpha>A</alpha></t:entry>
					<t:entry><beta>B</beta></t:entry>
					<t:entry><gamma>C</gamma></t:entry>
					<t:entry><delta>D</delta></t:entry>
				</t:default>
			</t:parameter>
			
			<t:parameter name="Example_List_Boolean" multiple="true" type="boolean">
				<t:default>
					<t:entry value="false"/>
					<t:entry value="true"/>
				</t:default>
			</t:parameter>
			
			<t:parameter name="Example_List_Integer" multiple="true" type="integer">
				<t:default>
					<t:entry value="2"/>
					<t:entry value="3"/>
					<t:entry value="5"/>
				</t:default>
			</t:parameter>
			
			<t:parameter name="Example_List_Float" multiple="true" type="float">
				<t:default>
					<t:entry value="-0.0"/>
					<t:entry value="-1.1"/>
					<t:entry value="-2.2"/>
				</t:default>
			</t:parameter>
			
			<t:parameter name="Example_List_Date" multiple="true" type="date">
				<t:default>
					<t:entry value="2011-11-11"/>
					<t:entry value="2012-12-12"/>
				</t:default>
			</t:parameter>
			
		</t:parameters>
	</t:head>
	<t:body>
		
		<t:foreach var='entry' values='$Example_List_String'>
			<t:value expr='entry' />
		</t:foreach>
		
		<t:foreach var='entry' values='$Example_List_Xml'>
			<t:value expr='entry' />
		</t:foreach>
		
		<t:foreach var='entry' values='$Example_List_Boolean'>
			<t:value expr='entry' />
		</t:foreach>
		
		<t:foreach var='entry' values='$Example_List_Integer'>
			<t:value expr='entry' />
		</t:foreach>
		
		<t:foreach var='entry' values='$Example_List_Float'>
			<t:value expr='entry' />
		</t:foreach>
		
		<t:foreach var='entry' values='$Example_List_Date'>
			<t:value expr='entry' />
		</t:foreach>
		
	</t:body>
</t:template>
