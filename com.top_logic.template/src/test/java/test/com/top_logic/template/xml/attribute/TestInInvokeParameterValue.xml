<?xml version='1.0' encoding='utf-8'?>

<t:template xmlns:t="http://www.top-logic.com/ns/template/1.0">
	<t:head>
		<t:settings output-format="text" />
		<t:parameters>
			
			<t:parameter name="An_Attribute" type="string" default="Example Value" />
			
		</t:parameters>
	</t:head>
	<t:body>
		<t:invoke template="TestInInvoke.xml">
			
			<t:parameter name="Example_Attribute" value="${$An_Attribute}" />
			
		</t:invoke>
	</t:body>
</t:template>
