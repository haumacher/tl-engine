<?xml version='1.0' encoding='utf-8'?>

<t:template xmlns:t="http://www.top-logic.com/ns/template/1.0">
	<t:head>
		<t:settings output-format="text" />
		<t:types>
			
			<t:type name="Example_Structure">
				<t:attributes>
					<t:attribute name="Left_Attribute" type="string">
						<t:default>
							Left Value
						</t:default>
					</t:attribute>
					<t:attribute name="Right_Attribute" type="string">
						<t:default>
							Right Value
						</t:default>
					</t:attribute>
				</t:attributes>
			</t:type>
			
		</t:types>
		<t:parameters>
			
			<t:parameter name="Example_Parameter" type="Example_Structure" />
			
		</t:parameters>
	</t:head>
	<t:body>
		
		<t:value expr="$Example_Parameter.Left_Attribute" />
		<t:value expr="$Example_Parameter.Right_Attribute" />
		
	</t:body>
</t:template>
