<?xml version="1.0" encoding="utf-8"?>

<!-- 
	Definition of the users in Top-Logic
-->

<objectlist xmlns="http://www.top-logic.com/ns/meta/5.8">

	<!-- MetaObjects -->

	<metaobjects>
		<metaobject object_name="user" db_name="TL_USER">
			<attributes>
				<mo_attribute att_name="username" att_type="String" db_size="128" db_name="CN" mandatory="true" />
				<mo_attribute att_name="givenName" att_type="String" db_size="128" db_name="GIVEN_NAME" mandatory="false" />
				<mo_attribute att_name="initials" att_type="String" db_size="32" db_name="INITIALS" mandatory="false" />
				<mo_attribute att_name="sn" att_type="String" db_size="128" db_name="SN" mandatory="false" />
				<mo_attribute att_name="personalTitle" att_type="String" db_size="32" db_name="TITLE" mandatory="false" />
				<mo_attribute att_name="telephoneNumber" att_type="String" db_size="64" db_name="PHONE" mandatory="false" />
				<mo_attribute att_name="mobile" att_type="String" db_size="64" db_name="MOBILE" mandatory="false" />
				<mo_attribute att_name="otherTelephone" att_type="String" db_size="64" db_name="OTHER_PHONE" mandatory="false" />
				<mo_attribute att_name="privateNumber" att_type="String" db_size="64" db_name="PRIVATE_PHONE" mandatory="false" />
				<mo_attribute att_name="displayName" att_type="String" db_size="512" db_name="DISPLAY_NAME" mandatory="false" />
				<mo_attribute att_name="orgUnit" att_type="String" db_size="254" db_name="ORG_UNIT" mandatory="false" />
				<mo_attribute att_name="description" att_type="String" db_size="254" db_name="DESCRIPTION" mandatory="false" />
				<mo_attribute att_name="userpassword" att_type="String" db_size="254" db_name="USERPASSWORD" mandatory="true" />
				<mo_attribute att_name="customerName" att_type="String" db_size="254" db_name="CUSTOMER" mandatory="false" />
				<mo_attribute att_name="userrole" att_type="String" db_size="254" db_name="USERROLE" mandatory="false" />
				<mo_attribute att_name="mail" att_type="String" db_size="254" db_name="MAIL" mandatory="false" />
				<mo_attribute att_name="otherMailbox" att_type="String" db_size="254" db_name="OTHER_MAIL" mandatory="false" />
				<mo_attribute att_name="dataDeviceID" att_type="String" db_size="254" db_name="DATA_ACCESS_DEVICE_ID" mandatory="false" />
			</attributes>
			<primary-key>
		   		<index-parts>
			        <index_part name="username"/>
		        </index-parts>
			</primary-key>
		</metaobject>

	</metaobjects>
</objectlist>
