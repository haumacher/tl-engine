<?xml version="1.0" encoding="utf-8" ?>

<t:template
	xmlns:config="http://www.top-logic.com/ns/config/6.0"
	xmlns:t="http://www.top-logic.com/ns/template/1.0"
>
	<t:head>
		<t:settings output-format="xml"/>
		<t:parameters>
			<t:parameter name="Login"
				type="string"
			/>
			<t:parameter name="Vorname"
				type="string"
			/>
			<t:parameter name="Nachname"
				type="string"
			/>
			<t:parameter name="Passwort"
				type="string"
			/>
			<t:parameter name="Email"
				default=""
				type="string"
			/>
			<t:parameter name="Titel"
				default=""
				type="string"
			/>
		</t:parameters>
	</t:head>
	<t:body>
		<action class="com.top_logic.layout.scripting.runtime.action.ActionChainOp">
			<actions>
				<action class="com.top_logic.layout.scripting.runtime.action.FuzzyGotoActionOp"
					user-id="root"
				>
					<component config:interface="com.top_logic.layout.scripting.recorder.ref.ui.FuzzyComponentNaming$Name"
						tab-path="Administration > Zug채nge"
					/>
				</action>
				<action class="com.top_logic.layout.scripting.runtime.action.CommandActionOp"
					command-implementation-comment="com.top_logic.tool.boundsec.OpenModalDialogCommandHandler"
					command-label="Neu..."
					command-name="displayDialog_newPersonDialog"
					component-implementation-comment="com.top_logic.contact.layout.person.related.EditRelatedPersonComponent"
					component-name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#AdminEditPerson"
					user-id="root"
				>
					<arguments/>
				</action>
				<action class="com.top_logic.layout.scripting.action.FormInput$Op"
					user-id="root"
				>
					<field config:interface="com.top_logic.layout.form.FuzzyFormMemberNaming$Name"
						label="Benutzername"
					>
						<component config:interface="com.top_logic.layout.scripting.recorder.ref.ui.FuzzyComponentNaming$Name"
							name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#NewPerson"
							tab-path=""
						/>
					</field>
					<value config:interface="com.top_logic.layout.scripting.recorder.ref.value.StringNaming$Name"
						value="${$Login}"
					/>
				</action>
				<action class="com.top_logic.layout.scripting.action.FormInput$Op"
					user-id="root"
				>
					<field config:interface="com.top_logic.layout.form.FuzzyFormMemberNaming$Name"
						label="Vorname"
					>
						<component config:interface="com.top_logic.layout.scripting.recorder.ref.ui.FuzzyComponentNaming$Name"
							name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#NewPerson"
							tab-path=""
						/>
					</field>
					<value config:interface="com.top_logic.layout.scripting.recorder.ref.value.StringNaming$Name"
						value="${$Vorname}"
					/>
				</action>
				<action class="com.top_logic.layout.scripting.action.FormInput$Op"
					user-id="root"
				>
					<field config:interface="com.top_logic.layout.form.FuzzyFormMemberNaming$Name"
						label="Nachname"
					>
						<component config:interface="com.top_logic.layout.scripting.recorder.ref.ui.FuzzyComponentNaming$Name"
							name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#NewPerson"
							tab-path=""
						/>
					</field>
					<value config:interface="com.top_logic.layout.scripting.recorder.ref.value.StringNaming$Name"
						value="${$Nachname}"
					/>
				</action>
				<action class="com.top_logic.layout.scripting.action.FormInput$Op"
					user-id="root"
				>
					<field config:interface="com.top_logic.layout.form.FuzzyFormMemberNaming$Name"
						label="Titel"
					>
						<component config:interface="com.top_logic.layout.scripting.recorder.ref.ui.FuzzyComponentNaming$Name"
							name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#NewPerson"
							tab-path=""
						/>
					</field>
					<value config:interface="com.top_logic.layout.scripting.recorder.ref.value.StringNaming$Name"
						value="${$Titel}"
					/>
				</action>
				<action class="com.top_logic.layout.scripting.runtime.action.CommandActionOp"
					command-implementation-comment="com.top_logic.contact.layout.person.related.NewRelatedPersonCommandHandler"
					command-label="Erstellen"
					command-name="newPerson"
					component-implementation-comment="com.top_logic.contact.layout.person.related.NewRelatedPersonComponent"
					component-name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#NewPerson"
					user-id="root"
				>
					<arguments/>
				</action>
				<action class="com.top_logic.layout.scripting.runtime.action.CommandActionOp"
					command-implementation-comment="com.top_logic.layout.form.component.RestrictedDefaultSaveCommandHandler"
					command-label="Speichern"
					command-name="defaultSave"
					component-implementation-comment="com.top_logic.contact.layout.person.related.EditRelatedPersonComponent"
					component-name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#AdminEditPerson"
					user-id="root"
				>
					<arguments/>
				</action>
				<action class="com.top_logic.layout.scripting.runtime.action.CommandActionOp"
					command-implementation-comment="com.top_logic.tool.boundsec.OpenModalDialogCommandHandler"
					command-label="Passwort 채ndern"
					command-name="displayDialog_AdminChangePasswordDialog"
					component-implementation-comment="com.top_logic.contact.layout.person.related.EditRelatedPersonComponent"
					component-name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#AdminEditPerson"
					user-id="root"
				>
					<arguments/>
				</action>
				<action class="com.top_logic.layout.scripting.action.FormInput$Op"
					user-id="root"
				>
					<field config:interface="com.top_logic.layout.form.FuzzyFormMemberNaming$Name"
						label="Passwortwiederholung"
					>
						<component config:interface="com.top_logic.layout.scripting.recorder.ref.ui.FuzzyComponentNaming$Name"
							name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#AdminChangePassword"
							tab-path=""
						/>
					</field>
					<value config:interface="com.top_logic.layout.scripting.recorder.ref.value.StringNaming$Name"
						value="${$Passwort}"
					/>
				</action>
				<action class="com.top_logic.layout.scripting.action.FormInput$Op"
					user-id="root"
				>
					<field config:interface="com.top_logic.layout.form.FuzzyFormMemberNaming$Name"
						label="Neues Passwort"
					>
						<component config:interface="com.top_logic.layout.scripting.recorder.ref.ui.FuzzyComponentNaming$Name"
							name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#AdminChangePassword"
							tab-path=""
						/>
					</field>
					<value config:interface="com.top_logic.layout.scripting.recorder.ref.value.StringNaming$Name"
						value="${$Passwort}"
					/>
				</action>
				<action class="com.top_logic.layout.scripting.runtime.action.CommandActionOp"
					command-implementation-comment="com.top_logic.knowledge.gui.layout.person.ChangePasswordComponent$ApplyPasswordCommand"
					command-label="Passwort 채ndern"
					command-name="changePassword"
					component-implementation-comment="com.top_logic.knowledge.gui.layout.person.ChangePasswordComponent"
					component-name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#AdminChangePassword"
					user-id="root"
				>
					<arguments/>
				</action>

				<action class="com.top_logic.layout.scripting.runtime.action.CommandActionOp"
					command-implementation-comment="com.top_logic.layout.form.component.RestrictedSwitchEditCommandHandler"
					command-label="Bearbeiten"
					command-name="switchToAJAXEdit"
					component-implementation-comment="com.top_logic.contact.layout.person.related.EditRelatedPersonComponent"
					component-name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#AdminEditPerson"
					user-id="root"
				>
					<arguments/>
				</action>
				<action class="com.top_logic.layout.scripting.action.FormInput$Op"
					user-id="root"
				>
					<field config:interface="com.top_logic.layout.form.FuzzyFormMemberNaming$Name"
						label="E-Mail"
					>
						<component config:interface="com.top_logic.layout.scripting.recorder.ref.ui.FuzzyComponentNaming$Name"
							name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#AdminEditPerson"
							tab-path="Administration > Zug채nge > Benutzerkonten"
						/>
					</field>
					<value config:interface="com.top_logic.layout.scripting.recorder.ref.value.StringNaming$Name"
						value="${$Email}"
					/>
				</action>
				<action class="com.top_logic.layout.scripting.runtime.action.CommandActionOp"
					command-implementation-comment="com.top_logic.layout.form.component.RestrictedDefaultSaveCommandHandler"
					command-label="Speichern"
					command-name="defaultSave"
					component-implementation-comment="com.top_logic.contact.layout.person.related.EditRelatedPersonComponent"
					component-name="com.top_logic/admin/security/accounts/adminPersonsView.layout.xml#AdminEditPerson"
					user-id="root"
				>
					<arguments/>
				</action>
			</actions>
		</action>
	</t:body>
</t:template>