<?xml version="1.0"?>

<objectlist>

<knowledgeobject object_type="Person" object_name="rootPerson">
	<ko_attribute att_name="name" att_value="root"/>
	<ko_attribute att_name="dataDeviceID" att_value="defaultDataAccessID"/>
	<ko_attribute att_name="authDeviceID" att_value="defaultAuthenticationID"/>		
</knowledgeobject>

<knowledgeobject object_type="Group" object_name="rootGroup">
	<ko_attribute att_name="name" att_value="root"/>
	<ko_attribute att_name="isSystem" att_value="true"/>		
</knowledgeobject>

<knowledgeassociation object_type="definesGroup" object_name="definesRootGroup">
	<ko_attribute att_name="source" att_value="rootPerson"/>
	<ko_attribute att_name="dest" att_value="rootGroup"/>
</knowledgeassociation>


</objectlist>
