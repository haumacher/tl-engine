<application xmlns:config="http://www.top-logic.com/ns/config/6.0">
	<!-- 
		Configuration overlay that is loaded when the application is booted during deployment 
		to generated scripts and styles.		
	-->
	<services>
		<config service-class="com.top_logic.gui.JSFileCompiler">
			<instance
				is-deployed="false"
				always-check="true"
				original-names="false"
			/>
		</config>
		<config service-class="com.top_logic.gui.ThemeFactory">
			<instance
				is-deployed="false"
			/>
		</config>
	</services>
</application>